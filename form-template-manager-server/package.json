{
  "name": "form-template-manager-server",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"ts,node,gql\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run graphql-schema-watch\"",
    "serve": "nodemon --delay 2500ms dist/index.js",
    "watch-ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "graphql-json": "apollo-codegen introspect-schema src/schema/schema.graphqls --output src/schema/schema.json",
    "graphql-ts": "gql-gen --file src/schema/schema.json --template typescript --out ./src/schema/schema.ts",
    "graphql-schema": "npm run graphql-json && npm run graphql-ts",
    "graphql-schema-watch": "nodemon --exec \"npm run graphql-schema\" --watch ./src/schema --ext graphqls",
    "prettier": "prettier --single-quote --write \"src/**/*.{ts,tsx}\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.16",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/express-graphql": "0.0.34",
    "@types/graphql": "^0.11.5",
    "@types/lodash": "^4.14.78",
    "@types/node-fetch": "^1.6.7",
    "@types/winston": "^2.3.6",
    "@types/zen-observable": "^0.5.3",
    "bluebird": "^3.5.1",
    "castv2-client": "^1.2.0",
    "chromecast-js": "^0.1.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.5.1",
    "iconv-lite": "^0.4.19",
    "internal-ip": "^3.0.1",
    "lodash": "^4.17.4",
    "magnet-uri": "^5.1.7",
    "mdns": "^2.3.3",
    "node-fetch": "^1.7.3",
    "opensubtitles-api": "^4.0.0",
    "peerflix": "^0.37.0",
    "server-destroy": "^1.0.1",
    "srt2vtt": "^1.3.1",
    "typescript": "^2.5.3",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "apollo-codegen": "^0.17.0",
    "concurrently": "^3.5.0",
    "graphql-code-generator": "^0.8.13",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "ts-node": "^3.3.0"
  }
}
